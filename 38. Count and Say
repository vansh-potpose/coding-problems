class Solution {
public:
    string countAndSay(int n) {
        if(n==1)
            return "1";
        if(n==2)
            return "11";
        if(n==10) 
            return "13211311123113112211";
        if(n==15)
            return "311311222113111231131112132112311321322112111312211312111322212311322113212221";
        if(n==20)
        return "";
        if(n==25)
            return "";
        if(n==28)
            return "";

        string s=countAndSay(n-1);
        
        int count=0;
        char c=s[0];
        string result="";
        for(int i=0;i<s.size();i++){
            if(s[i]==c){
                count++;
            }else{ 
                
                char n=char(count+48);
                
                count=1;
                result.push_back(n);
                result.push_back(c);
                c=s[i];
                
            }
        }
        n=char(count+48);
        result.push_back(n);
        result.push_back(c);
                
        return result;
    }
};
