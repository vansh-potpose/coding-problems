#include <iostream>
#include <numeric> // For std::gcd
using namespace std;

int main() {
    int t;
    cin >> t; // Read the number of test cases
    while (t--) {
        int n, m;
        cin >> n >> m; // Read the two integers
        cout << gcd(n, m) << endl; // Output the GCD of n and m
    }
    return 0;
}
