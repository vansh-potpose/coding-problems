#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t; // Read the number of test cases
    while (t--) {
        long long int x, y;
        cin >> x >> y; // Read the values of x and y
        if (x * x == 2 * y) {
            cout << "Yes" << endl; // Output "Yes" if the condition is true
        } else {
            cout << "No" << endl; // Output "No" otherwise
        }
    }
    return 0;
}
