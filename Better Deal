#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t; // Read the number of test cases
    while (t--) {
        int a, b;
        cin >> a >> b; // Read the values of a and b
        if (100 - a < 200 - 2 * b) {
            cout << "First" << endl; // Print "First" if the first condition is true
        } else if (100 - a > 200 - 2 * b) {
            cout << "Second" << endl; // Print "Second" if the second condition is true
        } else {
            cout << "Both" << endl; // Print "Both" if both conditions are equal
        }
    }
    return 0;
}
