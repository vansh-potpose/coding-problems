class MyHashSet {
public:
    
    MyHashSet():a(1000001)  {
    }
    
    void add(int key) {
        a[key]=true;
    }
    
    void remove(int key) {
        a[key]=false;
    }
    
    bool contains(int key) {
         return a[key];
    }
    private:
    vector<bool> a;
};

/**
 * Your MyHashSet object will be instantiated and called as such:
 * MyHashSet* obj = new MyHashSet();
 * obj->add(key);
 * obj->remove(key);
 * bool param_3 = obj->contains(key);
 */
