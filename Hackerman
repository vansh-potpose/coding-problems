#include <iostream>
#include <vector>

using namespace std;

int main() {
    int t;
    cin >> t; // Read the number of test cases
    while (t--) {
        int a, b;
        cin >> a >> b; // Read the two integers
        int sum = a + b; // Calculate the sum
        
        // Check if the sum is in the specified list
        if (sum == 2 || sum == 3 || sum == 5 || sum == 7 || sum == 11) {
            cout << "Alice" << endl; // Print "Alice" if the condition is met
        } else {
            cout << "Bob" << endl; // Print "Bob" otherwise
        }
    }
    return 0; // Return 0 to indicate successful execution
}
